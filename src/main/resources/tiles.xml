<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE tiles-definitions PUBLIC  "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"  "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>

    <definition name="main" template="/WEB-INF/views/layouts/layouts-tiles.jsp">
        <put-attribute name="header" value="/WEB-INF/views/layouts/header.jsp" />
        <put-attribute name="content" value="" />
    </definition>


    <definition name="/*/*/*/*/*.m" extends="main">
        <put-attribute name="content" value="/WEB-INF/views/{1}/{2}/{3}/{4}/{5}.jsp" />
    </definition>
    <definition name="/*/*/*/*.m" extends="main">
        <put-attribute name="content" value="/WEB-INF/views/{1}/{2}/{3}/{4}.jsp" />
    </definition>
    <definition name="/*/*/*.m" extends="main">
        <put-attribute name="content" value="/WEB-INF/views/{1}/{2}/{3}.jsp" />
    </definition>
    <definition name="/*/*.m" extends="main">
        <put-attribute name="content" value="/WEB-INF/views/{1}/{2}.jsp" />
    </definition>
    <definition name="/*.m" extends="main">
        <put-attribute name="content" value="/WEB-INF/views/{1}.jsp" />
    </definition>
   <definition name="*.m" extends="main">
        <put-attribute name="content" value="/WEB-INF/views/{1}/.jsp" />
    </definition>




</tiles-definitions>
